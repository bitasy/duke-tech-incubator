<div>
<h1 id="bonk-header">Duke Tech Incubator</h1>
<div class="search">
    <input type="text" name="search" placeholder="Search...">
</div>

<h3 align="center">Projects</h3>
<div id="constrainer2">
    <table style="border: 1px solid black">
        <tr>
            <th>Project Name</th>
            <th>Project Description</th>
        </tr>

        {{#each projects}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.description}}</td>
        </tr>
        {{/each}}
    </table>
</div>
</div>